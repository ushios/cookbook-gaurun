[core]
port = "<%=@core['port']%>"
workers = <%=@core['workers']%>
queues = <%=@core['queues']%>
notification_max = <%=@core['notification_max']%>

[api]
push_uri = "<%=@api['push_uri']%>"
stat_go_uri = "<%=@api['stat_go_uri']%>"
stat_app_uri = "<%=@api['stat_app_uri']%>"
config_app_uri = "<%=@api['config_app_uri']%>"

[android]
apikey = "<%=@android['apikey']%>"
enabled = <%=@android['enabled']%>
timeout = <%=@android['timeout']%> # sec
retry_max = <%=@android['retry_max']%>

[ios]
pem_cert_path = "<%=@ios['pem_cert_path']%>"
pem_key_path = "<%=@ios['pem_key_path']%>"
sandbox = <%=@ios['sandbox']%>
enabled = <%=@ios['enabled']%>
keepalive_max = <%=@ios['keepalive_max']%>
keepalive_idle_timeout = <%=@ios['keepalive_idle_timeout']%>
retry_max = <%=@ios['retry_max']%>
timeout_error = <%=@ios['timeout_error']%> # msec

[log]
access_log = "<%=@log['access_log']%>"
error_log = "<%=@log['error_log']%>"
level = "<%=@log['level']%>"
